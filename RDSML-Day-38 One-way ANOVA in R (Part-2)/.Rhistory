# One way anova
# Importing the dataset
PlantGrowth <- PlantGrowth
View(PlantGrowth)
# Importing the dplyr library
library(dplyr)
stats <- PlantGrowth %>%
group_by(group)  %>%
summarise(
mean = mean(weight),
sd = sd(weight),
se = sd(weight)/sqrt(n())
)
print(stats)
# Creating group wise box plots
boxplot(weight ~ group,
data = PlantGrowth,
main = "PlantGrowth data",
ylab = "Dried weight of plants",
col = "lightgray"
)
# Visualizing the data using mean plots
install.packages("ggpubr")
library(ggpubr)
ggline(PlantGrowth,
x = "group",
y = "weight",
add = c("mean_se", "jitter"))
# Running the one-way ANOVA test
anova <- aov(weight ~ group, data = PlantGrowth)
Summary <- anova
summary(anova)
# Tukey multiple pairwise-comparison
TukeyHSD(anova)
# Tukey multiple pairwise-comparison
TukeyHSD(anova)
# Homogeneity of variances
plot(anova, 1)
library(car)
leveneTest(weight ~ group, data = PlantGrowth)
leveneTest(weight ~ group, data = PlantGrowth)
# ANNOVA test with no assumption of equal variances
oneway.test(weight ~ group, data = PlantGrowth)
# Pairwise t-test with no assumption of equal variances
pairwise.t.test(PlantGrowth$weight, PlantGrowth$group,
p.adjust.method = "BH", pool.sd = FALSE)
# Checking for normal distribution
plot(anova, 2)
# Checking for normal distribution
plot(anova, 3)
# Checking for normal distribution
plot(anova, 2)
# Extracting the residuals
aov_res <- residuals(object = anova)
# Extracting the residuals
aov_res <- residuals(object = anova)
print(aov_res)
# Running the Shapiro-Wilk test
shapiro.test(aov_res)
